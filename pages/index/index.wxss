.search-container {
  background-color: #fff;
}

.search {
  height: 120rpx;
  padding-top: 70rpx;
  position: relative;
  width: 100vw;
  /* height: 50rpx; */
  display: flex;
  box-sizing: border-box;
  padding-left: 26rpx;
  background-color: #fff;
}

.search-title {
  margin-right: 40rpx;
  left: 0;
  font-weight: bold;
  font-size: 16px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.search input {
  display: block;
  box-sizing: border-box;
  background: #F7F8FA;
  height: 62rpx;
  border-radius: 20rpx;
  padding-left: 20rpx;
  line-height: 62rpx;
  font-size: 12px;
}